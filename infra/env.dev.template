# ============================================
# Configuration DEV - Winner Machine
# ============================================
# Copiez ce fichier en .env pour le développement local

# Base de données PostgreSQL
POSTGRES_USER=winner_machine
POSTGRES_PASSWORD=winner_machine_dev
POSTGRES_DB=winner_machine
N8N_DB_NAME=n8n

# Application Backend
APP_ENV=dev
ENVIRONMENT=development
DEBUG=true
LOG_LEVEL=INFO
SECRET_KEY=dev-secret-key-change-in-production

# API Keys (optionnel pour dev avec mock)
KEEPA_API_KEY=
AMAZON_SP_API_CLIENT_ID=
AMAZON_SP_API_CLIENT_SECRET=
KEYBUZZ_API_KEY=

# n8n Configuration
N8N_BASIC_AUTH_ACTIVE=true
N8N_BASIC_AUTH_USER=admin
N8N_BASIC_AUTH_PASSWORD=admin
N8N_ENCRYPTION_KEY=change-me-in-production

